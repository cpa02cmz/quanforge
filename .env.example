# Environment Variables Template
# Copy this file to .env.local for local development

# AI Configuration
VITE_API_KEY=your_google_gemini_api_key_here

# Supabase Configuration (Optional - will use LocalStorage if not provided)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Read Replica Configuration (Optional - for read scaling)
VITE_SUPABASE_READ_REPLICA_URL=your_supabase_read_replica_url
VITE_SUPABASE_READ_REPLICA_ANON_KEY=your_supabase_read_replica_anon_key

# Twelve Data API for real-time market data (Optional)
VITE_TWELVE_DATA_API_KEY=your_twelve_data_api_key

# Application Configuration
VITE_APP_NAME=QuantForge AI
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://quanforge.ai

# Production Optimizations
VITE_ENABLE_BUNDLE_ANALYZER=false
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PRELOAD=true
VITE_ENABLE_PREFETCH=true

# Performance Monitoring
VITE_ENABLE_WEB_VITALS=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Edge Configuration
VITE_EDGE_RUNTIME=false
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_EDGE_FUNCTIONS=true

# Edge Optimization Configuration
EDGE_METRICS_ENDPOINT=/api/edge/metrics
EDGE_CACHE_WARMUP_INTERVAL=300000
EDGE_SECURITY_THRESHOLD=0.1
EDGE_CONNECTION_WARMING=true
EDGE_REGION_AFFINITY=true
EDGE_CACHE_TTL=3600
EDGE_MAX_CONNECTIONS=3
EDGE_MIN_CONNECTIONS=1
EDGE_HEALTH_CHECK_INTERVAL=15000
EDGE_WARMUP_CONCURRENT=3
EDGE_METRICS_SAMPLE_RATE=0.05

# Vercel Specific
VERCEL_ENV=production
VERCEL_URL=quanforge.ai
VERCEL_REGION=global

# Supabase Optimization
SUPABASE_POOL_SIZE=4
SUPABASE_CONNECTION_TIMEOUT=800
SUPABASE_QUERY_TIMEOUT=3000
SUPABASE_CACHE_TTL=180000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ANALYTICS_ID=your_analytics_id

# Development/Production
NODE_ENV=development

# Critical Security Configuration
# API Endpoints
BINANCE_WS_URL=wss://stream.binance.com:9443/ws
TWELVE_DATA_WS_URL=wss://ws.twelvedata.com/v1/quotes
OPENAI_API_BASE_URL=https://api.openai.com/v1

# Rate Limiting Configuration
AI_RATE_LIMIT_RPM=10
DEFAULT_RATE_LIMIT_RPM=30
EDGE_RATE_LIMIT_RPS=10
RATE_LIMIT_REQUESTS=100

# Encryption Security
ENCRYPTION_ALGORITHM=AES-256-GCM
KEY_ROTATION_INTERVAL=43200000
CSRF_TOKEN_EXPIRY=3600000

# WebSocket Configuration
WS_BINANCE_MAX_RECONNECT_ATTEMPTS=10
WS_BINANCE_BASE_RECONNECT_DELAY=1000
WS_BINANCE_MAX_RECONNECT_DELAY=30000
WS_TWELVE_MAX_RECONNECT_ATTEMPTS=10
WS_TWELVE_BASE_RECONNECT_DELAY=1000
WS_TWELVE_MAX_RECONNECT_DELAY=30000

# Database Configuration
DB_MAX_RETRIES=5
DB_RETRY_DELAY=500
DB_BACKOFF_MULTIPLIER=1.5
DB_MAX_DELAY=10000
DB_CACHE_TTL=900000
DB_CACHE_MAX_SIZE=200
DB_QUERY_LIMIT=100
DB_BATCH_SIZE=10

# AI Model Configuration
DEFAULT_GEMINI_MODEL=gemini-3-pro-preview
DEFAULT_OPENAI_MODEL=gpt-4
DEFAULT_ANALYSIS_MODEL=gemini-2.5-flash
DEFAULT_AI_TEMPERATURE=0.7

# AI Performance Configuration
MAX_PROMPT_LENGTH=10000
MIN_PROMPT_LENGTH=10
AI_MAX_CONTEXT_CHARS=100000
AI_MIN_HISTORY_CHARS=1000
AI_MAX_CODE_LENGTH=30000

# AI Cache Configuration
AI_CACHE_MAX_SIZE=100
AI_CACHE_TTL=300000
STRATEGY_CACHE_SIZE=100
STRATEGY_CACHE_TTL=300000
ENHANCED_CACHE_SIZE=200
MQL5_CACHE_SIZE=300
MQL5_RESPONSE_TTL=900000
AI_CONTEXT_CACHE_SIZE=20
AI_CONTEXT_CACHE_TTL=300000

# Security Origins
ALLOWED_ORIGINS=https://quanforge.ai,https://www.quanforge.ai,http://localhost:3000,http://localhost:5173

# General Security
MAX_PAYLOAD_SIZE=5242880

# Rate Limiting Windows
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Edge Rate Limiting
EDGE_RATE_LIMIT_ENABLED=true
EDGE_RATE_LIMIT_BURST=20

# Region Blocking
REGION_BLOCKING_ENABLED=true
BLOCKED_REGIONS=CN,RU,IR,KP

# Bot Detection
BOT_DETECTION_ENABLED=true

# AI Worker Timeouts
AI_TIMEOUT_DEFAULT=30000
AI_TIMEOUT_CONTEXT=10000
AI_TIMEOUT_RESPONSE=5000
AI_TIMEOUT_EXTRACTION=3000
AI_TIMEOUT_FORMATTING=3000
AI_TIMEOUT_GENERATION=60000
AI_TIMEOUT_PARSING=5000
AI_TIMEOUT_HEALTH=2000

# Database Timeouts
DB_TIMEOUT_QUERY=30000
DB_TIMEOUT_CONNECTION=10000

# Cache TTLs
CACHE_DEFAULT_TTL=300000
CACHE_ANALYTICS_TTL=60000
CACHE_METRICS_TTL=600000

# Monitoring Configuration
MONITORING_MAX_METRICS=100
MONITORING_MAX_ERRORS=50
MONITORING_BATCH_SIZE=10

# Edge Runtime
EDGE_RUNTIME_ENABLED=true
EDGE_REGIONS=hkg1,iad1,sin1,fra1,sfo1,arn1,gru1

# Edge Cache TTLs
EDGE_CACHE_STATIC_TTL=31536000
EDGE_CACHE_HEALTH_TTL=30
EDGE_CACHE_ROBOTS_TTL=300
EDGE_CACHE_STRATEGIES_TTL=300
EDGE_CACHE_ANALYTICS_TTL=180

# Database Replicas
DB_REPLICAS_ENABLED=true
DB_REPLICA_REGIONS=us-east-1,eu-west-1,ap-southeast-1
DB_REPLICA_MAX_METRICS=1000
DB_FAILOVER_TIMEOUT=5000

# Real User Monitoring
RUM_ENABLED=true
RUM_SAMPLE_RATE=0.1
RUM_THRESHOLD_LCP=2500
RUM_THRESHOLD_FID=100
RUM_THRESHOLD_CLS=0.1
RUM_THRESHOLD_FCP=1800
RUM_THRESHOLD_TTFB=800
RUM_THRESHOLD_INP=200

# Circuit Breaker Configuration
CB_DB_FAILURE_THRESHOLD=5
CB_DB_TIMEOUT=10000
CB_DB_RESET_TIMEOUT=60000
CB_AI_FAILURE_THRESHOLD=3
CB_AI_TIMEOUT=15000
CB_AI_RESET_TIMEOUT=30000
CB_MD_FAILURE_THRESHOLD=7
CB_MD_TIMEOUT=5000
CB_MD_RESET_TIMEOUT=120000

# Feature Flags
FEATURE_EDGE_OPTIMIZATION=true
FEATURE_COMPRESSION=true
FEATURE_PREFETCH=true
FEATURE_PRELOAD=true
FEATURE_WEB_VITALS=true
FEATURE_ADVANCED_SECURITY=true
FEATURE_MULTI_REGION=true
FEATURE_AI_OPTIMIZATION=true