{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "/"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Edge-Cache",
          "value": "HIT"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600, stale-while-revalidate=120"
        },
        {
          "key": "Edge-Control",
          "value": "cache-maxage=300"
        },
        {
          "key": "X-Edge-API",
          "value": "optimized"
        },
        {
          "key": "Edge-Cache-Tag",
          "value": "api, dynamic, version-1"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.vercel-insights.com https://*.supabase.co https://www.googletagmanager.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; img-src 'self' data: https: https://*.supabase.co https://www.google-analytics.com; font-src 'self' data: https://fonts.gstatic.com; connect-src 'self' https://*.supabase.co https://googleapis.com https://www.google-analytics.com https://region1.google-analytics.com; frame-src 'none'; frame-ancestors 'none'; object-src 'none'; base-uri 'self'; form-action 'self'; upgrade-insecure-requests;"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), payment=(), usb=(), magnetometer=(), gyroscope=(), accelerometer=(), ambient-light-sensor=()"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "devCommand": "npm run dev",
  "functions": {
    "api/**/*.ts": {
      "runtime": "edge",
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "EDGE_RUNTIME": "true",
      "ENABLE_EDGE_CACHING": "true",
      "EDGE_CACHE_TTL": "1800",
      "ENABLE_BROTLI": "true",
      "ENABLE_GZIP": "true",
      "VITE_ENABLE_EDGE_RUNTIME": "true",
      "VITE_ENABLE_SERVICE_WORKER": "true",
      "VITE_ENABLE_COMPRESSION": "true",
      "VITE_ENABLE_WEB_VITALS": "true",
      "VITE_ENABLE_PERFORMANCE_MONITORING": "true",
      "VITE_ENABLE_BUNDLE_ANALYZER": "true",
      "VITE_ENABLE_PRELOAD": "true",
      "VITE_ENABLE_PREFETCH": "true",
      "ENABLE_EDGE_METRICS": "true",
      "EDGE_METRICS_ENDPOINT": "/api/edge-metrics",
      "PERFORMANCE_SAMPLE_RATE": "0.05",
"SUPABASE_CONNECTION_POOL_SIZE": "2",
        "SUPABASE_QUERY_TIMEOUT": "1500",
        "SUPABASE_CACHE_TTL": "120000",
      "CHUNK_SIZE_LIMIT": "150000",
      "ENABLE_CODE_SPLITTING": "true",
      "ENABLE_BUNDLE_ANALYSIS": "true",
      "OPTIMIZE_CRITICAL_PATH": "true"
    }
  },
  "crons": [],
  "rewrites": [
    {
      "source": "/api/supabase/(.*)",
      "destination": "/api/supabase/$1"
    },
    {
      "source": "/cdn/(.*)",
      "destination": "https://assets.quanforge.ai/$1"
    }
  ]
}