{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "/"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Edge-Cache",
          "value": "HIT"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=600, stale-while-revalidate=120"
        },
        {
          "key": "Edge-Control",
          "value": "cache-maxage=300"
        },
        {
          "key": "X-Edge-API",
          "value": "optimized"
        },
        {
          "key": "Edge-Cache-Tag",
          "value": "api, dynamic, version-1"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdn.vercel-insights.com https://*.supabase.co https://www.googletagmanager.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; img-src 'self' data: https: https://*.supabase.co https://www.google-analytics.com; font-src 'self' data: https://fonts.gstatic.com; connect-src 'self' https://*.supabase.co https://googleapis.com https://www.google-analytics.com https://region1.google-analytics.com https://generativelanguage.googleapis.com; frame-src 'none'; frame-ancestors 'none'; object-src 'none'; base-uri 'self'; form-action 'self'; upgrade-insecure-requests; require-trusted-types-for 'script';"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), payment=(), usb=(), magnetometer=(), gyroscope=(), accelerometer=(), ambient-light-sensor=(), interest-cohort=(), browsing-topics=()"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "regions": ["hkg1", "iad1", "sin1", "fra1", "sfo1", "arn1", "gru1", "cle1"],
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci",
  "devCommand": "npm run dev",
  "functions": {
    "api/**/*.ts": {
      "runtime": "edge",
      "regions": ["hkg1", "iad1", "sin1", "fra1", "sfo1"],
      "maxDuration": 15,
      "memory": 512,
      "includeFiles": ["public/**/*"],
      "excludeFiles": ["**/*.test.*", "**/*.spec.*"],
      "cache": "max-age=600, s-maxage=1800, stale-while-revalidate=600",
      "environment": {
        "EDGE_CACHE_ENABLED": "true",
        "SUPABASE_POOL_SIZE": "4",
        "EDGE_REGION_AFFINITY": "true",
        "CONNECTION_WARMING": "true",
        "EDGE_KEEP_WARM": "true",
        "PREWARM_CONCURRENT": "3",
        "EDGE_MEMORY_POOL": "true",
        "EDGE_OPTIMIZATION_LEVEL": "maximum",
        "PREDICTIVE_CACHING": "true",
        "EDGE_METRICS_ENABLED": "true",
        "EDGE_CACHE_TTL": "1800",
        "EDGE_MAX_CONNECTIONS": "8",
        "EDGE_MIN_CONNECTIONS": "2",
        "EDGE_HEALTH_CHECK_INTERVAL": "15000",
        "EDGE_CACHE_STRATEGY": "intelligent",
        "ENABLE_EDGE_CACHE": "true",
        "CONNECTION_POOL_SIZE": "8",
        "NEXT_TELEMETRY_DISABLED": "1",
        "VERCEL_ANALYTICS_ENABLED": "true",
        "EDGE_FUNCTION_TIMEOUT": "55000",
        "EDGE_WARMUP_STRATEGY": "predictive",
        "EDGE_DEDUPLICATION_ENABLED": "true",
        "EDGE_COMPRESSION_ENABLED": "true",
        "EDGE_BROTLI_ENABLED": "true",
        "EDGE_QUIC_ENABLED": "true",
        "EDGE_HTTP3_ENABLED": "true",
        "EDGE_RATE_LIMITING": "adaptive",
        "EDGE_SECURITY_HEADERS": "enhanced",
        "EDGE_PERFORMANCE_MONITORING": "realtime",
        "EDGE_OPTIMIZATION_SERVICE": "enabled",
        "EDGE_CONNECTION_DRAINING": "true",
        "EDGE_ADAPTIVE_WARMING": "true",
        "EDGE_PREDICTIVE_PREFETCH": "true",
        "EDGE_SMART_CACHING": "true",
        "EDGE_HEALTH_MONITORING": "continuous",
        "EDGE_BROTLI_LEVEL": "11",
        "EDGE_BROTLI_WINDOW": "22",
        "EDGE_BROTLI_CHUNK": "0",
        "EDGE_COMPRESSION_THRESHOLD": "1024",
        "EDGE_GZIP_LEVEL": "9",
        "EDGE_COMPRESSION_STRATEGY": "adaptive",
        "EDGE_MIN_COMPRESSION_SIZE": "512"
      }
    },
    "api/edge/**/*.ts": {
      "runtime": "edge",
      "regions": ["hkg1", "iad1", "sin1", "fra1", "sfo1", "arn1", "gru1", "cle1"],
      "maxDuration": 15,
      "memory": 512,
      "cache": "max-age=300, s-maxage=900, stale-while-revalidate=300",
      "environment": {
        "EDGE_OPTIMIZATION_LEVEL": "maximum",
        "EDGE_CACHE_STRATEGY": "aggressive",
        "EDGE_PERFORMANCE_MONITORING": "realtime"
      }
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "EDGE_RUNTIME": "true",
      "ENABLE_EDGE_CACHING": "true",
      "EDGE_CACHE_TTL": "900",
      "ENABLE_BROTLI": "true",
      "ENABLE_GZIP": "true",
      "VITE_ENABLE_EDGE_RUNTIME": "true",
      "VITE_ENABLE_SERVICE_WORKER": "true",
      "VITE_ENABLE_COMPRESSION": "true",
      "VITE_ENABLE_WEB_VITALS": "true",
      "VITE_ENABLE_PERFORMANCE_MONITORING": "true",
      "VITE_ENABLE_BUNDLE_ANALYZER": "false",
      "VITE_ENABLE_PRELOAD": "true",
      "VITE_ENABLE_PREFETCH": "true",
      "ENABLE_EDGE_METRICS": "true",
      "EDGE_METRICS_ENDPOINT": "/api/edge-metrics",
      "PERFORMANCE_SAMPLE_RATE": "0.1",
"SUPABASE_CONNECTION_POOL_SIZE": "12",
        "SUPABASE_QUERY_TIMEOUT": "7500",
        "SUPABASE_CACHE_TTL": "180000",
"CHUNK_SIZE_LIMIT": "150000",
        "ENABLE_CODE_SPLITTING": "true",
        "ENABLE_BUNDLE_ANALYSIS": "true",
        "ENABLE_TREE_SHAKING": "aggressive",
        "ENABLE_DEAD_CODE_ELIMINATION": "aggressive",
      "OPTIMIZE_CRITICAL_PATH": "true",
      "NEXT_TELEMETRY_DISABLED": "1",
      "VERCEL_ANALYTICS_ENABLED": "true",
      "BUILD_OPTIMIZATION": "maximum",
      "TREESHAKING_LEVEL": "maximum",
      "COMPRESSION_LEVEL": "maximum",
      "ENABLE_SMART_CACHING": "true",
      "ENABLE_PREDICTIVE_PREFETCH": "true",
      "ENABLE_ADVANCED_MINIFICATION": "true",
      "ENABLE_DEAD_CODE_ELIMINATION": "true",
      "ENABLE_MODULE_CONCATENATION": "true",
      "BROTLI_COMPRESSION_LEVEL": "11",
      "BROTLI_WINDOW_SIZE": "22",
      "BROTLI_CHUNK_SIZE": "0",
      "COMPRESSION_THRESHOLD": "1024",
      "GZIP_COMPRESSION_LEVEL": "9",
      "ENABLE_ADAPTIVE_COMPRESSION": "true",
      "MIN_COMPRESSION_SIZE": "512",
      "EDGE_WEB_WORKER_ENABLED": "true",
      "PREDICTIVE_PRELOADING_ENABLED": "true"
    }
  },
  "crons": [],
  "experimental": {
    "edgeFunctions": {
      "warm": true,
      "maxConcurrency": 1000,
      "incrementalCache": true,
      "streaming": true
    },
    "allowedHosts": [
      "quanforge.ai",
      "*.quanforge.ai",
      "*.supabase.co"
    ]
  },
  "rewrites": [
    {
      "source": "/api/supabase/(.*)",
      "destination": "/api/supabase/$1"
    },
    {
      "source": "/cdn/(.*)",
      "destination": "https://assets.quanforge.ai/$1"
    }
]
}