{
  "name": "quantforge-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced MQL5 Trading Robot Generator powered by AI",
  "keywords": [
    "mql5",
    "metatrader",
    "trading",
    "ai",
    "robot",
    "expert-advisor",
    "forex",
    "algorithmic-trading"
  ],
  "author": "QuantForge AI",
  "homepage": "https://quanforge.ai",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build --mode edge",
    "build:optimize": "npm run build:production && npm run optimize:bundle && npm run optimize:edge",
    "build:analyze": "ANALYZE=true npm run build:production && npm run analyze:bundle",
    "optimize:bundle": "node scripts/optimize-bundle.cjs",
    "optimize:edge": "node scripts/enhanced-edge-optimizer.cjs",
    "analyze:bundle": "npx @vercel/webpack-bundle-analyzer dist/static/js/*.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist node_modules/.vite",
    "vercel-build": "npm run build:production",
    "deploy:staged": "npm run build:production && npm warm:edge && vercel --prebuilt",
    "deploy:prod": "vercel --prod",
    "warm:edge": "node scripts/warm-edge-cache.js",
    "performance-check": "npm run build:production && npm run typecheck && npm run lint",
    "generate:preload-list": "node scripts/generate-preload-list.js"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.3.0",
    "lz-string": "^1.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^25.0.0",
    "terser": "^5.34.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vitest": "^4.0.15"
  }
}
