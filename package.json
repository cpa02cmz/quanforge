{
  "name": "quantforge-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced MQL5 Trading Robot Generator powered by AI",
  "keywords": [
    "mql5",
    "metatrader",
    "trading",
    "ai",
    "robot",
    "expert-advisor",
    "forex",
    "algorithmic-trading"
  ],
  "author": "QuantForge AI",
  "homepage": "https://quanforge.ai",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:edge": "vite build --mode edge",
    "build:production": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "clean": "rm -rf dist node_modules/.vite",
    "vercel-build": "npm run build:production",
    "bundle-size": "npm run build && echo 'Bundle size analysis complete'",
    "edge-optimize": "npm run build:edge && npm run bundle-size",
    "performance-check": "npm run build && npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.3.0",
    "lz-string": "^1.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^1.3.0",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^25.0.0",
    "terser": "^5.34.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.0.0"
  }
}
