{
  "name": "quantforge-ai",
  "private": true,
  "version": "1.6.0",
  "type": "module",
  "description": "Advanced MQL5 Trading Robot Generator powered by AI",
  "keywords": [
    "mql5",
    "metatrader",
    "trading",
    "ai",
    "robot",
    "expert-advisor",
    "forex",
    "algorithmic-trading"
  ],
  "author": "QuantForge AI",
  "homepage": "https://quanforge.ai",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:profile": "vite build --mode profile",
    "build:benchmark": "npm run clean && npm run build && npm run build:analyze",
    "build:edge": "NODE_ENV=production vite build --mode edge",
    "build:edge-analyze": "ANALYZE=true vite build --mode edge",
    "build:edge-profile": "vite build --mode edge --profile",
    "build:production": "npm run build:edge",
    "build:edge-optimized": "NODE_ENV=production vite build --mode edge && npm run optimize:bundle && npm run optimize:edge",
    "build:with-preload": "npm run build:edge-optimized && npm run generate:preload-list",
    "build:parallel": "npm run build:assets && npm run build:bundle",
    "build:assets": "vite build --mode assets-only",
    "build:bundle": "vite build --mode bundle-only",
    "build:optimized": "npm run build:parallel && npm run optimize:bundle",
    "optimize:bundle": "node scripts/optimize-bundle.cjs",
    "optimize:edge": "node scripts/enhanced-edge-optimizer.cjs",
    "deploy:staged": "npm run build:edge && npm run warm:edge && vercel --prebuilt",
    "edge:performance-test": "npm run build:edge && npm run test:edge",
    "edge:warmup": "node scripts/warm-edge-cache.js",
    "edge:warmup:enhanced": "node scripts/enhanced-edge-warming.cjs",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:performance": "vitest run --reporter=verbose --config=vite.performance.config.ts",
    "test:edge": "vitest run --config=vite.edge.config.ts",
    "clean": "rm -rf dist node_modules/.vite",
    "vercel-build": "npm run build:production",
    "bundle-size": "node scripts/bundle-size-monitor.js",
    "bundle-size:check": "node scripts/bundle-size-monitor.js --check",
    "bundle-size:validate": "node scripts/validate-bundle-size.js",
    "build:validate": "npm run build && npm run bundle-size:validate",
    "edge-optimize": "npm run build:edge && npm run test:performance",
    "deploy:edge": "vercel --prod",
    "cache:warm": "node scripts/warm-edge-cache.js",
    "bundle:optimize": "npx @vercel/webpack-bundle-analyzer dist/static/js/*.js",
    "performance-check": "npm run build && npm run typecheck && npm run lint",
    "health-check": "echo '=== Running Health Checks ===' && npm run typecheck && echo '✓ TypeScript OK' && npm run lint && echo '✓ Lint OK' && npm run test:run && echo '✓ Tests OK' && npm run build && echo '✓ Build OK' && echo '=== All Health Checks Passed ==='",
    "analyze:bundle": "npx @vercel/webpack-bundle-analyzer dist/static/js/*.js",
    "check:bundle-size": "node scripts/check-bundle-size.cjs",
    "generate:preload-list": "node scripts/generate-preload-list.js"
  },
  "dependencies": {
    "@google/genai": "1.41.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/dompurify": "^3.0.5",
    "@vercel/kv": "^3.0.0",
    "dompurify": "^3.3.1",
    "lz-string": "^1.5.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.3.1",
    "@types/node": "25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/ui": "4.0.18",
    "chrome-launcher": "^1.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "0.5.0",
    "jsdom": "^28.1.0",
    "lighthouse": "^12.8.2",
    "playwright": "^1.58.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.34.1",
    "typescript": "~5.9.3",
    "vite": "^6.4.1",
    "vitest": "4.0.18"
  }
}
